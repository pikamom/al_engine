[flake8]
exclude =
    .git,
    __pycache__
max-line-length = 120
ignore = E203, E266, E402, E501, W503, F403, F401
max-complexity = 18
select = E101,E121,E122,E124,E125,E126,E127,E128,E131,E201,E202,E221,E222,E225,E226,E227,E231,E241,E251,E252,E261,E262,E265,E271,E302,E303,E305,E502,E713,E722,F811,F821,F841,W191,W504
aggressive=3
